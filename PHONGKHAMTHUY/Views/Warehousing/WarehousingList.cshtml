
@{
    ViewBag.Title = "WarehousingList";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Breadcrumb = "Nhập kho";
    ViewBag.PageText = "Nhập kho thuốc/vật tư";
}

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <div class="row justify-content-between">
                    <h4 class="card-title">Nhập kho thuốc - vật tư</h4>
                    <button data-toggle="modal" data-target="#modal-add" id="addPet" class="btn btn-primary btn-lg rounded-circle">
                        <i class="fas fa-plus"></i>
                    </button>
                </div>
                <h6 class="card-title mt-3">
                </h6>
                <div class="table-responsive">
                    <table id="zero_config" class="table table-striped table-bordered no-wrap">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>TÊN VẬT NUÔI</th>
                                <th>CHỦ SỠ HỮU</th>
                                <th>LOÀI</th>
                                <th>GIỐNG</th>
                                <th>TUỔI</th>
                                <th>MÀU SẮC</th>
                                <th>CÂN NẶNG</th>
                                <th>GIỚI TÍNH</th>
                                <th>NGÀY SINH</th>
                                <th>NGÀY TẠO</th>
                                <th>SỬA</th>
                                <th>XÓA</th>
                            </tr>
                        </thead>
                        <tbody>
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

